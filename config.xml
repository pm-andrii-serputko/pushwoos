<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aserputko.notifications.pushwoosh" version="1.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Notifications!!!</name>
    <description>
        Notifications app - make life simple!!!!
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Serputko Team
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.5.0" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="false" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="deployment-target" value="7.0" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#FFFFFF" />
    <preference name="StatusBarStyle" value="default" />
    
    <gap:plugin name="org.apache.cordova.battery-status" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.media-capture" />
    <gap:plugin name="org.apache.cordova.console" />
    <gap:plugin name="org.apache.cordova.contacts" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-motion" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.file" />
    <gap:plugin name="org.apache.cordova.file-transfer" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.globalization" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="com.pushwoosh.plugins.pushwoosh" />
    <gap:plugin name="com.phonegap.plugin.statusbar" />
    
    <gap:platform name="ios" />

    <icon src="images/phonegap/icon.png" />

    <platform name="ios">
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="images/phonegap/icon/ios/icon-60@3x.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="images/phonegap/icon/ios/icon-60.png" width="60" height="60" />
        <icon src="images/phonegap/icon/ios/icon-60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="images/phonegap/icon/ios/icon-76.png" width="76" height="76" />
        <icon src="images/phonegap/icon/ios/icon-76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="images/phonegap/icon/ios/icon-40.png" width="40" height="40" />
        <icon src="images/phonegap/icon/ios/icon-40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="images/phonegap/icon/ios/icon.png" width="57" height="57" />
        <icon src="images/phonegap/icon/ios/icon@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="images/phonegap/icon/ios/icon-72.png" width="72" height="72" />
        <icon src="images/phonegap/icon/ios/icon-72@2x.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="images/phonegap/icon/ios/icon-small.png" width="29" height="29" />
        <icon src="images/phonegap/icon/ios/icon-small@2x.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="images/phonegap/icon/ios/icon-50.png" width="50" height="50" />
        <icon src="images/phonegap/icon/ios/icon-50@2x.png" width="100" height="100" />
    </platform>

    <!-- Optional splash screen image, e.g.: -->
    <gap:splash src="images/phonegap/screen/ios/splash480.png" gap:platform="ios" width="480" height="320" />
    <gap:splash src="images/phonegap/screen/ios/splash320x480.png" gap:platform="ios" width="320" height="480" />

    <gap:splash src="images/phonegap/screen/ios/splash960.png" gap:platform="ios" width="960" height="640" />
    <gap:splash src="images/phonegap/screen/ios/splash640x960.png" gap:platform="ios" width="640" height="960" />

    <gap:splash src="images/phonegap/screen/ios/splash_iphone5.png" gap:platform="ios" width="1136" height="640" />
    <gap:splash src="images/phonegap/screen/ios/splash640x1136.png" gap:platform="ios" width="640" height="1136" />

    <gap:splash src="images/phonegap/screen/ios/splash.png" gap:platform="ios" width="1024" height="768" />
    <gap:splash src="images/phonegap/screen/ios/splash768x1024.png" gap:platform="ios" width="768" height="1024" />

    <gap:splash src="images/phonegap/screen/ios/splash568.png" gap:platform="ios" width="568" height="320" />
    <gap:splash src="images/phonegap/screen/ios/splash320.png" gap:platform="ios" width="320" height="568" />

    <gap:splash src="images/phonegap/screen/ios/splash1334.png" gap:platform="ios" width="1334" height="750" />
    <gap:splash src="images/phonegap/screen/ios/splash750x1334.png" gap:platform="ios" width="750" height="1334" />

    <gap:splash src="images/phonegap/screen/ios/splash2208.png" gap:platform="ios" width="2208" height="1242" />
    <gap:splash src="images/phonegap/screen/ios/splash1242x2208.png" gap:platform="ios" width="1242" height="2208" />

    <gap:splash src="images/phonegap/screen/ios/splash2048x1536.png" gap:platform="ios" width="2048" height="1536" />
    <gap:splash src="images/phonegap/screen/ios/splash1536x2048.png" gap:platform="ios" width="1536" height="2048" />

    <access origin="*" />
</widget>
